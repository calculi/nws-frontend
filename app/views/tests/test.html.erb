<h1>This is test page</h1>

<div ng-controller="DropdownCtrl">
  <a dropdown-toggle="#dropdown-example-1">Click me for a dropdown!</a>
  <ul id="dropdown-example-1" class="f-dropdown">
    <li ng-repeat="choice in items">
      <a>{{choice}}</a>
    </li>
  </ul>
  <br>
  <br>

  <a class="button split">
    Split Button
    <span dropdown-toggle="#dropdown-example-2"></span>
  </a>
  <ul id="dropdown-example-2" class="f-dropdown">
    <li ng-repeat="choice in items">
      <a>{{choice}}</a>
    </li>
  </ul>
</div>

<hr>

<div ng-controller="TabCtrl">
	<tabset type="navType">
		<tab ng-repeat="tab in tabs" heading="{{tab.title}}" active="tab.active">
	      {{tab.content}}
	    </tab>
	</tabset>
</div>